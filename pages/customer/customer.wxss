.imagesize{
  position: relative;
  text-align: center;
  margin:20rpx;
  margin-bottom: 50rpx;
}
.imagesize image { 
  width:100%;
  height:300rpx;
}
.imagesize .child {
  width: 100%;
  height: 50rpx;  
  position: absolute;
  left: 0;
  top: 0;
  font-size:32rpx;
  padding: 5rpx;
  background:#FFCC01;
  text-align: left;
  color:#666666;
}